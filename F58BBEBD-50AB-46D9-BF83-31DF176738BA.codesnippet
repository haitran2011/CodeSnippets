<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>IDECodeSnippetCompletionPrefix</key>
	<string>vm</string>
	<key>IDECodeSnippetCompletionScopes</key>
	<array>
		<string>TopLevel</string>
	</array>
	<key>IDECodeSnippetContents</key>
	<string>import ReactiveCocoa
import TinyCoordinator

class &lt;#ViewModel#&gt; {
    lazy var globalDataMetric: MutableProperty&lt;TCGlobalDataMetric&gt; = {
        self.fetchDataAction.rac_values(TCGlobalDataMetric(sectionDataMetrics: []))
    }()
    
    lazy var error: MutableProperty&lt;NSError&gt; = {
        self.fetchDataAction.rac_errors(NSError.defaultError())
    }()
    
    private lazy var fetchDataAction: Action&lt;Void, TCGlobalDataMetric, NSError&gt; = {
        return Action {
            // FIXME:
            &lt;#API#&gt;().asyncProducer
                .map { (result) -&gt; TCGlobalDataMetric in
                    let sectionDataMetric = TCSectionDataMetric(itemsData: [result])
                    let globalDataMetric = TCGlobalDataMetric(sectionDataMetrics: [sectionDataMetric])
                    return globalDataMetric
            }
        }
    }()
    
    func fetchData() {
        fetchDataAction.apply().start()
    }
    
    deinit {
        loggingPrint("")
    }
}</string>
	<key>IDECodeSnippetIdentifier</key>
	<string>F58BBEBD-50AB-46D9-BF83-31DF176738BA</string>
	<key>IDECodeSnippetLanguage</key>
	<string>Xcode.SourceCodeLanguage.Swift</string>
	<key>IDECodeSnippetTitle</key>
	<string>ViewModel</string>
	<key>IDECodeSnippetUserSnippet</key>
	<true/>
	<key>IDECodeSnippetVersion</key>
	<integer>2</integer>
</dict>
</plist>
